{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "id",
        "username",
        "password",
        "firstName",
        "lastName",
        "email",
        "accessCode"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                1
            ]
        },
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "parentExample"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "parentPassword"
            ]
        },
        "first_name": {
            "type": "string",
            "default": "",
            "title": "The first_name Schema",
            "examples": [
                "John"
            ]
        },
        "last_name": {
            "type": "string",
            "default": "",
            "title": "The last_name Schema",
            "examples": [
                "Doe"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "john@doe.com"
            ]
        },
        "access_code": {
            "type": "integer",
            "default": 0,
            "title": "The access_code Schema",
            "examples": [
                1234
            ]
        }
    },
    "examples": [{
        "id": 1,
        "username": "parentExample",
        "password": "parentPassword",
        "first_name": "John",
        "last_name": "Doe",
        "email": "john@doe.com",
        "access_code": 1234
    }]
}